<script>
    function onl() {
        $('#forma').form();
        <%if (submitted) { %>
        swal({
            type: "success",
            title: "Undertaking Submitted",
            html: `<div style="font-weight:bold;">Please note the following details for future reference
            <br/><br/>Reference id: <%=id%><br/><br/>Name: <%=name%>
            <br/><br/>Email: <%=email%><br/><br/>Mobile: <%=mobile%></div>`,
            confirmButtonColor: '#d33'
        });
        <% } %>
    }
</script>
